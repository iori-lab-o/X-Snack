{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowJs": true,
    "target": "ES2020",
    "lib": ["ES2020"]
  },
  "exclude": ["node_modules"]
}
